  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#828997",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation",
      "scope": [
        "comment.documentation",
        "meta.attribute.rust"
      ],
      "settings": {
        "foreground": "#81a1c1"
      }
    },
    {
      "name": "Wildcard and unused",
      "scope": [
        "wildcard.comment",
        "unused.comment"
      ],
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.control", // Elixir
        "storage.type.class",
        "storage.modifier",
        "keyword.other.rust",
        "storage.type.rust",
        "keyword.other.elm",
        "keyword.import.elm",
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Class definition",
      "scope": [
        "entity.name.type.module", // Elixir mod def
        "entity.name.type.class", // JS
        "entity.name.type.rust",
        "support.other.module.elm",
        "entity.other.inherited-class.js"
      ],
      "settings": {
        "foreground": "#E86AFF"
      }
    },
    {
      "name": "Class call",
      "scope": [
        "variable.other.constant", // Elixir mod def
        "variable.parameter",
        "storage.class.std.rust",
        "support.class.ruby",
        "constant.other.elm",
        "meta.export.default.js",
        "support.class.builtin.js",
        "support.constant.math.js",
        "entity.name.type.js",
        "variable.other.readwrite.alias.js"
      ],
      "settings": {
        "foreground": "#DDAAFF"
      }
    },
    {
      "name": "Function definition",
      "scope": [
        "entity.name.function", // Elixir func def
        "punctuation.definition.variable", // Elixir @
        "punctuation.definition.parameters",
        "storage.type.function", // JS
        "keyword.other.fn.rust",
        "meta.definition.function.js"
      ],
      "settings": {
        "foreground": "#5DA9F6"
      }
    },
    {
      "name": "Function call",
      "scope": [
        "punctuation.section.function", // Elixir func call
        "support.function",
        "punctuation.separator.method", // Elixir .
        "variable.other.anonymous",
        "meta.function.expression",
        "entity.name.function.js",
        "punctuation.accessor.js",
        "meta.brace.round.js"
      ],
      "settings": {
        "foreground": "#A5C7FF"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.parameter.function.ruby",
        "variable.other.readwrite.js",
        "variable.parameter.js"
      ],
      "settings": {
        "foreground": "#dedacf"
      }
    },
    {
      "name": "Special method (import, etc)",
      "scope": [
        "keyword.other.special-method", // Elixir, Ruby
        "keyword.other.unsafe.rust",
        "keyword.control.pseudo-method.ruby",
        "keyword.operator.new.js",
        "keyword.control.import.js",
        "keyword.control.from.js",
        "keyword.control.export.js",
        "keyword.control.default.js"
      ],
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "Symbol",
      "scope": [
        "constant.other.symbol",
        "variable.other.readwrite",
        "punctuation.definition.variable.ruby",
        "meta.object-literal.key.js",
        "storage.modifier.lifetime.rust",
        "meta.function.type-declaration.elm",
        "storage.type.elm",
        "constant.language.symbol.ruby"
      ],
      "settings": {
        "foreground": "#7fc1ca"
      }
    },
    {
      "name": "Object property",
      "scope": [
        "storage.type.core.rust",
        "meta.type_params.rust",
        "support.constant.elm",
        "support.variable.property.js",
        "variable.other.object.property.js",
        "variable.other.property.js",
        "support.variable.property.dom.js"
      ],
      "settings": {
        "foreground": "#EEE5B2"
      }
    },
    {
      "name": "Constant (true, false, nil)",
      "scope": [
        "constant.language",
        "variable.language.elixir",
        "variable.language",
        "support.constant.core.rust",
        "variable.other.object.ruby"
      ],
      "settings": {
        "foreground": "#9e4fe6"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#83CE4D"
      }
    },
    {
      "name": "Single quote",
      "scope": [
        "support.function.variable.quoted.single.elixir",
        "string.quoted.single.ruby"
      ],
      "settings": {
        "foreground": "#ddf88f"
      }
    },
    {
      "name": "Interpolation",
      "scope": [
        "punctuation.section.embedded",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "Interpolation embedded",
      "scope": "source.elixir.embedded",
      "settings": {
        "foreground": "#DEDACF"
      }
    },
    {
      "name": "Regexp",
      "scope": [
        "string.regexp",
        "punctuation.section.regexp"
      ],
      "settings": {
        "foreground": "#00ead7"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FF615A"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d8a13b"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "storage.type.variable.ruby"
      ],
      "settings": {
        "foreground": "#EBD99C"
      }
    },
    {
      "name": "Separator",
      "scope": [
        "punctuation.section.array",
        "punctuation.section.scope",
        "punctuation.definition.dictionary",
        "punctuation.definition.array",
        "punctuation.separator",
        "punctuation.separator",
        "punctuation.section",
        "punctuation.terminator",
        "constant.language.empty-list.elm",
        "constant.language.unit.elm",
        "punctuation.definition.block.js",
        "meta.brace.square.js"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Html property name",
      "scope": [
        "entity.name.tag",
        "support.type.property-name",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#DDAAFF"
      }
    },
    {
      "name": "Html attribute name",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.name.tag.css",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#EBD99C"
      }
    },
    {
      "name": "Html property value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#83CE4D"
      }
    },
    {
      "name": "Html punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#57b3c1"
      }
    },
    {
      "name": "Html structure",
      "scope": "entity.name.tag.structure",
      "settings": {
        "foreground": "#dedacf"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.type.property-value",
        "meta.property-value"
      ],
      "settings": {
        "foreground": "#A5C7FF"
      }
    },
    {
      "name": "Css unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "React component",
      "scope": "support.class.component.js",
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markup heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#ebd99c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": [
        "markup.italic",
        "markup.bold"
      ],
      "settings": {
        "foreground": "#88c0d0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup link name",
      "scope": "string.other.link.title",
      "settings": {
        "foreground": "#a5c7ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup link",
      "scope": [
        "markup.underline.link",
        "constant.other.reference.link"
      ],
      "settings": {
        "foreground": "#ddaaff"
      }
    },
    {
      "name": "Markup list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#9a93e1"
      }
    },
    {
      "name": "Markup raw",
      "scope": [
        "markup.inline.raw",
        "markup.raw",
        "markup.quote",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#83ce4d"
      }
    }
  ]
}
