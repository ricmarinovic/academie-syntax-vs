{
  "name": "Academie Syntax",
  "type": "dark",
  "colors": {
    "activityBar.background": "#2F333D",
    "activityBar.foreground": "#D7DAE0",
    "activityBarBadge.background": "#4D78CC",
    "activityBarBadge.foreground": "#F8FAFD",
    "badge.background": "#282c34",
    "button.background": "#404754",
    "debugToolBar.background": "#21252b",
    "dropdown.background": "#1d1f23",
    "diffEditor.insertedTextBackground": "#00809B33",
    "dropdown.border": "#181A1F",
    "editor.background": "#282C34",
    "editor.foreground": "#DEDACF",
    "editorBracketMatch.border": "#83CE4D",
    "editorError.foreground": "#c24038",
    "editorGutter.addedBackground": "#B1E969",
    "editorGutter.deletedBackground": "#FF615A",
    "editorGutter.modifiedBackground": "#e5c07b",
    "editorMarkerNavigation.background": "#21252b",
    "editorRuler.foreground": "#abb2bf26",
    "editor.lineHighlightBackground": "#383E4A",
    "editor.selectionBackground": "#3E4451",
    "editorCursor.foreground": "#83CE4D",
    "editor.findMatchHighlightBackground": "#314365",
    "editorGroup.background": "#181A1F",
    "editorGroup.border": "#181A1F",
    "editorGroupHeader.tabsBackground": "#21252B",
    "editorIndentGuide.background": "#3B4048",
    "editorLineNumber.foreground": "#495162",
    "editorWhitespace.foreground": "#3B4048",
    "editorHoverWidget.background": "#21252B",
    "editorHoverWidget.border": "#181A1F",
    "editorSuggestWidget.background": "#21252B",
    "editorSuggestWidget.border": "#181A1F",
    "editorSuggestWidget.selectedBackground": "#2c313a",
    "editorWidget.background": "#21252B",
    "input.background": "#1d1f23",
    "list.activeSelectionBackground": "#2c313a",
    "list.activeSelectionForeground": "#d7dae0",
    "list.focusBackground": "#383E4A",
    "list.hoverBackground": "#292d35",
    "list.highlightForeground": "#C5C5C5",
    "list.inactiveSelectionBackground": "#2c313a",
    "list.inactiveSelectionForeground": "#d7dae0",
    "notification.background": "#21252b",
    "peekViewEditor.matchHighlightBackground": "#29244b",
    "scrollbarSlider.background": "#4E566680",
    "scrollbarSlider.activeBackground": "#747D9180",
    "scrollbarSlider.hoverBackground": "#5A637580",
    "sideBar.background": "#21252b",
    "sideBarSectionHeader.background": "#282c34",
    "statusBar.background": "#21252B",
    "statusBar.foreground": "#9da5b4",
    "statusBarItem.hoverBackground": "#2c313a",
    "tab.activeBackground": "#2c313a",
    "tab.border": "#181A1F",
    "tab.inactiveBackground": "#21252B",
    "titleBar.activeBackground": "#282c34",
    "titleBar.activeForeground": "#9da5b4",
    "titleBar.inactiveBackground": "#282C34",
    "titleBar.inactiveForeground": "#6B717D",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#313131",
    "terminal.ansiRed": "#FF615A",
    "terminal.ansiBrightRed": "#F58C80",
    "terminal.ansiGreen": "#83CE4D",
    "terminal.ansiBrightGreen": "#DDF88F",
    "terminal.ansiYellow": "#EBD99C",
    "terminal.ansiBrightYellow": "#EEE5B2",
    "terminal.ansiBlue": "#5DA9F6",
    "terminal.ansiBrightBlue": "#A5C7FF",
    "terminal.ansiMagenta": "#E86AFF",
    "terminal.ansiBrightMagenta": "#DDAAFF",
    "terminal.ansiCyan": "#57B3C1",
    "terminal.ansiBrightCyan": "#00ead7",
    "terminal.ansiWhite": "#DEDACF",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.background": "#22262d"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": "comment",
      "settings": {
        "foreground": "#828997",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Documentation",
      "scope": "comment.documentation, meta.attribute.rust",
      "settings": {
        "foreground": "#57B3C1"
      }
    },
    {
      "name": "Wildcard (_ on elixir)",
      "scope": "wildcard.comment",
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Unused (_var on elixir)",
      "scope": "unused.comment",
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class definition",
      "scope": "entity.name.type.module, entity.name.type.class, entity.other.inherited-class.js",
      "settings": {
        "foreground": "#E86AFF"
      }
    },
    {
      "name": "Class call",
      "scope": "variable.other.constant.elixir, variable.other.readwrite.alias.js, meta.export.default.js, support.class.builtin.js, support.constant.math.js, entity.name.type.js",
      "settings": {
        "foreground": "#DDAAFF"
      }
    },
    {
      "name": "Function definition",
      "scope": "entity.name.function, punctuation.definition.variable, punctuation.definition.parameters, storage.type.function,meta.definition.function.js, keyword.other.fn.rust",
      "settings": {
        "foreground": "#5DA9F6"
      }
    },
    {
      "name": "Function call",
      "scope": "variable.other.anonymous, punctuation.section.function, punctuation.separator.method, punctuation.accessor.js, support.function, meta.function.expression meta.brace.round, entity.name.function.js, meta.brace.round.js",
      "settings": {
        "foreground": "#A5C7FF"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.control, storage.type.class, storage.modifier, keyword.other.rust",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Variable",
      "scope": "variable.other.readwrite.js, variable.parameter.js",
      "settings": {
        "foreground": "#dedacf"
      }
    },
    {
      "name": "Constant",
      "scope": "variable.other.constant, storage.class.std.rust",
      "settings": {
        "foreground": "#E86AFF"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type.js",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Special method (import, etc)",
      "scope": "keyword.other.special-method, keyword.operator.new.js",
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "Readwrite",
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#5da9f6"
      }
    },
    {
      "name": "Symbol",
      "scope": "constant.other.symbol, meta.object-literal.key.js, support.variable.property.js, variable.other.object.property.js, variable.other.property.js, support.variable.property.dom.js",
      "settings": {
        "foreground": "#EEE5B2"
      }
    },
    {
      "name": "Constant (true, false, nil)",
      "scope": "constant.language, variable.language.elixir, variable.language, storage.type.core.rust",
      "settings": {
        "foreground": "#9E4FE6"
      }
    },
    {
      "name": "String",
      "scope": "string, string.quoted.double",
      "settings": {
        "foreground": "#83CE4D"
      }
    },
    {
      "name": "Single quote",
      "scope": "support.function.variable.quoted.single.elixir, string.quoted.single.ruby",
      "settings": {
        "foreground": "#ddf88f"
      }
    },
    {
      "name": "Interpolation",
      "scope": "punctuation.section.embedded, punctuation.definition.template-expression",
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "Interpolation embedded",
      "scope": "source.elixir.embedded",
      "settings": {
        "foreground": "#DEDACF"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#00ead7"
      }
    },
    {
      "name": "Character escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FF615A"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "name": "Operator arithmetic",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#EBD99C"
      }
    },
    {
      "name": "Separator",
      "scope": "punctuation.section.array, punctuation.section.scope, punctuation.definition.block.js, meta.brace.square.js, punctuation.definition.dictionary, punctuation.definition.array, punctuation.separator.object.elixir, punctuation.separator.comma.js",
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Html property name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#ddaaff"
      }
    },
    {
      "name": "Html property value",
      "scope": "support.type.property-value, meta.property-value, string.quoted.double.html",
      "settings": {
        "foreground": "#a5c7ff"
      }
    },
    {
      "name": "Css unit",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#F58C80"
      }
    },
    {
      "name": "Html structure",
      "scope": "entity.name.tag.structure",
      "settings": {
        "foreground": "#dedacf"
      }
    },
    {
      "name": "Html attribute name",
      "scope": "entity.other.attribute-name, support.type.property-name",
      "settings": {
        "foreground": "#ebd99c"
      }
    },
    {
      "name": "Html punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#57b3c1"
      }
    },
    {
      "name": "Markup heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#57B3C1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#ebd99c",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#ebd99c",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup link name",
      "scope": "string.other.link.title",
      "settings": {
        "foreground": "#a5c7ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup link",
      "scope": "markup.underline.link, constant.other.reference.link",
      "settings": {
        "foreground": "#ddaaff"
      }
    },
    {
      "name": "Markup list",
      "scope": "beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#9e4fe6"
      }
    },
    {
      "name": "Markup raw",
      "scope": "markup.inline.raw, markup.raw, markup.quote, markup.fenced_code.block",
      "settings": {
        "foreground": "#83ce4d"
      }
    }
  ]
}
